---
layout: layouts/base.njk
---

{{ content | safe }}

<div class="mx-auto">
  <div class=" flex flex-wrap">
  {% for image in collections[gallery] %}
      <div class="flex md:w-1/3 flex-wrap">
      <div class="w-full p-1">
        <img
          alt="gallery"
          class="block h-full w-full rounded-lg object-cover object-center"
          src="{{image.path}}" />
      </div>
    </div>
{% endfor %}
  </div>
</div>

